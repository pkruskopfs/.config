 Basic key bindings here
super + Return
        $TERMINAL
super + d
        dmenu_run
super + e
        $TERMINAL -e neomutt
super + n
        $TERMINAL -e newsboat
super + alt + m
        $TERMINAL -e ncmpcpp 
super + shift + l
        $TERMINAL -e slock
super + w
	$TERMINAL -e less -Srf ~/.local/share/weatherreport
super + Print
	flameshot gui
super + shift + a
				$TERMINAL -e ani-cli
super + p
				$TERMINAL -e pavucontrol
XF86AudioMute
				pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86Audio{Raise,Lower}Volume
				pactl set-sink-volume @DEFAULT_SINK@ {+,-}2%

super + {u,i}
				pactl set-sink-volume @DEFAULT_SINK@ {+,-}2%

super + shift + {u,i}
				pactl set-sink-volume @DEFAULT_SINK@ {+,-}10%

super + shift + d
				~/dmenuscripts/dmconf

super + alt + v
				$TERMINAL -e mbsync -a -c ~/.config/mbsync/config

super + shift + b
				$TERMINAL -e calibre --with-library ~/.local/share/Calibre\ Library/

super + shift+ f
				$TERMINAL -e lfub
