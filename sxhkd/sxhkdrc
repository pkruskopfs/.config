 Basic key bindings here
super + Return
        $TERMINAL
super + d
        dmenu_run
super + e
        $TERMINAL -e neomutt
super + n
        $TERMINAL -e newsboat
super + m
        $TERMINAL -e ncmpcpp 
super + shift + l
        $TERMINAL -e slock
super + w
	$TERMINAL -e less -Srf ~/.local/share/weatherreport
super + Print
	flameshot gui
super + {alt,ctrl,alt + ctrl} + XF86Eject
	sudo systemctl {suspend,reboot,poweroff}
