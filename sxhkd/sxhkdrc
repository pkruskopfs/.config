 Basic key bindings here
super + Return
        $TERMINAL
super + d
        dmenu_run
super + e
        $TERMINAL -e neomutt
super + n
        $TERMINAL -e newsboat
super + alt + m
        $TERMINAL -e ncmpcpp 
super + shift + l
        $TERMINAL -e slock
super + w
	$TERMINAL -e less -Srf ~/.local/share/weatherreport
super + Print
	flameshot gui
super + a
				$TERMINAL -e ani-cli
super + p
				$TERMINAL -e pavucontrol
XF86AudioMute
    pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86Audio{Raise,Lower}Volume
    pactl set-sink-volume @DEFAULT_SINK@ {+,-}2%

super + {k,j}
    pactl set-sink-volume @DEFAULT_SINK@ {+,-}2%

super + shift + {k,j}
    pactl set-sink-volume @DEFAULT_SINK@ {+,-}10%

